{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],  // если реакт не нужен
    "rules": {
        "semi": ["error", "never"],
        // "no-console": "off",
        // "no-plusplus": "off",
        "max-len": ["error", 150, {"ignoreUrls": true}],
        "no-tabs": "warn",
        "no-alert": "warn",
        "prefer-destructuring": "off"
    }
}